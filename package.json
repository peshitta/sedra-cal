{
  "name": "sedra-cal",
  "version": "1.0.3",
  "description": "Convert from Sedra 3 to CAL ASCII transliteration",
  "main": "build/sedra-cal.js",
  "mainMin": "build/sedra-cal.min.js",
  "module": "build/sedra-cal.esm.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "npm run doc && npm run rollup && npm run lint && npm run test",
    "test": "nyc mocha",
    "rollup": "rollup -c",
    "lint": "eslint src/*.js test/*.js",
    "doc": "jsdoc2md -t doc/readme.hbs src/main.js > README.md"
  },
  "nyc": {
    "include": [
      "build/sedra-cal.min.js"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "no-plusplus": 0,
      "comma-dangle": 0,
      "no-nested-ternary": 0,
      "arrow-parens": 0
    }
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peshitta/sedra-cal.git"
  },
  "keywords": [
    "Sedra",
    "CAL",
    "Peshitta",
    "Peshitto",
    "Sedra.js",
    "Sedrajs",
    "Aramaic",
    "Mapper",
    "Converter"
  ],
  "author": "Greg Borota",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peshitta/sedra-cal/issues"
  },
  "homepage": "https://github.com/peshitta/sedra-cal#readme",
  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.7.0",
    "jsdoc-to-markdown": "^3.0.0",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "rollup": "^0.49.2",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-uglify": "^2.0.1"
  }
}
