{
  "name": "sedra-cal",
  "version": "1.0.0",
  "description": "Convert between Sedra 3 and CAL ASCII transliterations",
  "main": "build/sedra-cal.js",
  "module": "build/sedra-cal.esm.js",
  "scripts": {
    "build": "npm run doc && npm run lint && npm run rollup && npm run min",
    "min": "uglifyjs ./build/sedra-cal.js -o ./build/sedra-cal.min.js -c dead_code,toplevel,comparisons,unsafe=true,unsafe_Func=true,unsafe_comps=true -m toplevel --comments",
    "rollup": "rollup -c",
    "lint": "eslint src/*.js",
    "doc": "jsdoc2md -t src/README.hbs src/*.js > README.md"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "no-plusplus": 0,
      "comma-dangle": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peshitta/sedra-cal.git"
  },
  "keywords": [
    "Sedra",
    "CAL",
    "Peshitta",
    "Peshitto",
    "Sedra.js",
    "Sedrajs",
    "Aramaic",
    "Mapper",
    "Converter"
  ],
  "author": "Greg Borota",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peshitta/sedra-cal/issues"
  },
  "homepage": "https://github.com/peshitta/sedra-cal#readme",
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.7.0",
    "jsdoc-to-markdown": "^3.0.0",
    "rollup": "^0.49.2",
    "rollup-plugin-buble": "^0.15.0",
    "uglify-js": "^3.0.28"
  }
}
